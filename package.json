        "vue-lazyload": "^1.2.6",
        "vue-router": "^3.0.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.4.0",
        "@vue/cli-plugin-eslint": "^3.4.0",
        "@vue/cli-service": "^3.4.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0",
        "file-loader": "^1.1.4",
        "html-webpack-plugin": "^2.30.1",
        "inject-loader": "^3.0.0",
