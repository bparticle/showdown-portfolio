    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "bundledDependencies": [
        "vue",
        "fullpage.js",
        "json-loader",
        "vue-fullpage.js",
        "vue-lazyload",
        "vue-router"
